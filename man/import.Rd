con <- system.file("extdata", "example.csv", package = "pipette")

## Row and column names enabled.
x <- import(con = con)
print(head(x))

## Row and column names disabled.
x <- import(con = con, rownames = FALSE, colnames = FALSE)
print(head(x))
}
